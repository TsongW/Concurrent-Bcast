    "mask = 0x1\n",
    "i = 0\n",
    "comm_size=8\n",
    "rank=7\n",
    "while (mask < comm_size):\n",
    "    dst = rank ^ mask\n",
    "\n",
    "    dst_tree_root = dst >> i\n",
    "    dst_tree_root <<= i\n",
    "\n",
    "    my_tree_root = rank >> i\n",
    "    my_tree_root <<= i\n",
    "    \n",
    "    print(my_tree_root, dst_tree_root)\n",
    "    \n",
    "    mask <<= 1\n",
    "    i+=1"
   ]
  }
